{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "source": "/api/(.*)",
      "destination": "/app.js"
    },
    {
      "source": "/config.js",
      "destination": "/public/dist/config.js"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/public/dist/assets/$1"
    },
    {
      "source": "/(.*\\.(js|css|svg|png|jpg|jpeg|gif|ico|woff|woff2|ttf|eot))",
      "destination": "/public/dist/$1"
    },
    {
      "source": "/",
      "destination": "/public/dist/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/public/dist/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}